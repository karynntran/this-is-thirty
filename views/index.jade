extends layout
script(src="/javascripts/landing.js")			
script(src="/javascripts/main.js")

block content
	div.container
		div.hero-container
			h1.hero-text this is thirty.
			p.hero-description my 30th year through a lens
		div.about-container
			ul.quick-glance-ul
				li.quick-glance-li.home-nav#homeNav &#9829;
				li.quick-glance-li#januaryNav JAN
				li.quick-glance-li#februaryNav FEB
				li.quick-glance-li#marchNav MAR
				li.quick-glance-li#aprilNav APR
				li.quick-glance-li#mayNav MAY
				li.quick-glance-li#juneNav JUN
				li.quick-glance-li#julyNav JUL
				li.quick-glance-li#augustNav AUG
				li.quick-glance-li#septemberNav SEP
				li.quick-glance-li#octoberNav OCT
				li.quick-glance-li#novemberNav NOV
				li.quick-glance-li#decemberNav DEC
			div.marker-container
				div#marker
			div.quick-description-container
				p.quick-description-text#quickDescriptionText Click on each month to learn more.
